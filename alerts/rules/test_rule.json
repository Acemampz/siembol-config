{
  "rule_name": "test_rule",
  "rule_version": 1,
  "rule_author": "demo",
  "rule_protection": {
    "max_per_hour": 100,
    "max_per_day": 10000
  },
  "rule_description": "test rule - is_alert is equal to true",
